      <ScrollPane fitToHeight="true" fitToWidth="true" prefHeight="400.0" prefWidth="900.0" styleClass="scroll-pane" stylesheets="@../runboard.css">
         <content>
            <GridPane fx:id="boardPane" alignment="CENTER" hgap="10.0" vgap="10.0">
              <columnConstraints>
                  <ColumnConstraints />
                  <ColumnConstraints />
              </columnConstraints>
              <rowConstraints>
                  <RowConstraints />
                  <RowConstraints />
                  <RowConstraints />
                  <RowConstraints />
              </rowConstraints>
               <padding>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
